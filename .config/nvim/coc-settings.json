{
  "diagnostic.virtualTextCurrentLineOnly": true,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.enableMessage": "jump",
  "diagnostic.enable": true,
  "diagnostic.enableHighlightLineNumber": true,
  "diagnostic.errorSign": "",
  "diagnostic.hintSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.warningSign": "",
  "diagnostic.separateRelatedInformationAsDiagnostics": false,
  "diagnostic.messageDelay": 4000,
  "diagnostic.messageTarget": "echo",
  "diagnostic.refreshOnInsertMode": false,
  "diagnostic.highlightOffset": 1000,
  "diagnostic.locationlistUpdate": true,
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextLines": 1,
  "diagnostic.maxWindowWidth": 30,
  "diagnostic.level": "warning",
  "diagnostic.virtualTextLineSeparator": " \\ ",

  "prettier.disableSuccessMessage": true,
  "prettier.singleQuote": true,
  "prettier.eslintIntegration": true,

  "suggest.completionItemKindLabels": {
    "keyword": "\uf1de",
    "variable": "\ue79b",
    "value": "\uf89f",
    "operator": "\u03a8",
    "constructor": "\uf0ad",
    "function": "\u0192",
    "reference": "\ufa46",
    "constant": "\uf8fe",
    "method": "\uf09a",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b",
    "property": "\ue624",
    "field": "\uf9be",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf723",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  },

  "snippets.priority": 500,
  "snippets.ultisnips.enable": true,
  "snippets.enableStatusItem": true,
  "snippets.shortcut": "\ue60b",

  "suggest.languageSourcePriority": 200,
  "suggest.maxPreviewWidth": 60,
  "suggest.enablePreselect": false,
  "suggest.snippetIndicator": "►",
  "suggest.floatEnable": true,
  "suggest.enablePreview": true,
  "suggest.removeDuplicateItems": true,
  "suggest.triggerAfterInsertEnter": true,
  "suggest.minTriggerInputLength": 2,
  "suggest.defaultSortMethod": "alphabetical",
  "suggest.maxCompleteItemCount": 100,
  "suggest.acceptSuggestionOnCommitCharacter": true,
  "suggest.autoTrigger": "always",
  "suggest.noselect": true,
  "suggest.timeout": 5000,
  "suggest.triggerCompletionWait": 500,
  "suggest.detailField": "abbr",
  "suggest.detailMaxLength": 100,

  "graphql.debug": true,
  "graphql.filetypes": ["graphql"],
  "sh.enable": true,
  "sh.highlightParsingErrors": false,
  "docker.enable": true,
  "solargraph.diagnostics": true,
  "solargraph.formatting": false,
  "solargraph.autoformat": false,

  "git.enableGutters": true,
  "git.changeRemovedSign.text": " ",
  "git.showCommitInFloating": true,
  "git.addedSign.hlGroup": "GitGutterAdd",
  "git.changedSign.hlGroup": "GitGutterChange",
  "git.removedSign.hlGroup": "GitGutterDelete",
  "git.topRemovedSign.hlGroup": "GitGutterDelete",
  "git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",
  "git.addGBlameToVirtualText": false,
  "git.blameUseRealTime": true,

  "tsserver.enable": true,
  "tsserver.reportStyleChecksAsWarnings": true,
  "tsserver.implicitProjectConfig.checkJs": true,

  "eslint.quiet": false,
  "eslint.codeAction.showDocumentation": {"enable": true},

  "javascript.validate.enable": false,
  "javascript.format.enabled": false,
  "javascript.suggest.autoImports": true,
  "javascript.showUnused": true,
  "javascript.updateImportsOnFileMove.enable": true,
  "javascript.referencesCodeLens.enable": true,
  "javascript.suggest.enabled": true,
  "javascript.suggest.names": true,
  "javascript.suggest.paths": true,
  "javascript.implementationsCodeLens.enable": true,

  "javascript.suggestionActions.enabled": true,
  "typescript.validate.enable": false,
  "typescript.suggest.enabled": false,
  "typescript.format.enabled": false,

  "codeLens.enable": false,
  "signature.enable": true,

  "list.insertMappings": {
    "<C-x>": "action:split",
    "<C-v>": "action:vsplit"
  },
  "list.normalMappings": {
    "s": "action:split",
    "v": "action:vsplit"
  },
  "list.source.files.command": "fd",
  "list.source.files.args": ["--type", "f"],

  "html.suggest.html5": true,

  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "wxss.validate": false,
  "styled-components.tags": ["styled", "css", "sty"],
  "stylelintplus.autoFixOnFormat": false,
  "stylelintplus.autoFixOnSave": false,
  "stylelintplus.cssInJs": false,
  "stylelintplus.enable": true,
  "stylelintplus.validateOnType": true,

  "yank.highlight.enable": true,
  "yank.highlight.duration": 200,

  "coc.preferences.useQuickfixForLocations": true,
  "coc.preferences.colorSupport": true,
  "coc.preferences.previewAutoClose": true,
  "coc.preferences.previewMaxHeight": 70,
  "coc.preferences.messageLevel": "more",
  "coc.preferences.highlightTimeout": 500,
  "coc.preferences.promptInput": true,
  "coc.preferences.enableFloatHighlight": true,
  "coc.preferences.rootPatterns": [".git", "package.json"],
  "coc.preferences.enableMarkdown": true,
  "coc.preferences.bracketEnterImprove": true,
  "coc.preferences.formatOnType": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "js",
    "jsx",
    "json",
    "javascript",
    "javascriptreact"
  ],

  "tsserver.enableJavascript": true,

  "diagnostic-languageserver.enable": true,
  "diagnostic-languageserver.filetypes": {
    "vim": "vint",
    "email": "languagetool",
    "markdown": ["write-good", "markdownlint"],
    "sh": "shellcheck",
    "elixir": ["mix_credo", "mix_credo_compile"],
    "eelixir": ["mix_credo", "mix_credo_compile"],
    "php": ["phpstan", "psalm"]
  },
  "diagnostic-languageserver.formatFiletypes": {
    "dart": "dartfmt",
    "elixir": "mix_format",
    "eelixir": "mix_format",
    "lua": "lua-format",
    "sh": "shfmt",
    "blade": "blade-formatter"
  },
  "diagnostic-languageserver.linters": {
    "vint": {
      "command": "vint",
      "debounce": 100,
      "args": ["--enable-neovim", "-"],
      "offsetLine": 0,
      "offsetColumn": 0,
      "sourceName": "vint",
      "formatLines": 1,
      "formatPattern": [
        "[^:]+:(\\d+):(\\d+):\\s*(.*)(\\r|\\n)*$",
        {
          "line": 1,
          "column": 2,
          "message": 3
        }
      ]
    }
  },
  "languageserver": {
    "graphql": {
      "command": "~/.nvm/versions/node/v14.8.0/bin/graphql-lsp",
      "filetypes": ["graphql"],
      "args": ["server"],
      "ignoredRootPaths": ["/", "~"]
    },
    "terraform": {
      "command": "terraform-lsp",
      "filetypes": ["terraform"],
      "initializationOptions": {}
    },
    "ccls": {
      "command": "ccls",
      "filetypes": ["c", "cpp", "objc", "objcpp"],
      "rootPatterns": [
        ".ccls",
        ".ccls-root",
        "compile_commands.json",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "initializationOptions": {
        "cache": {
          "directory": ".ccls-cache"
        }
      }
    }
  }
}
