52ccc0a3b371ccdc216996ea14f41300|file:///home/nicholas/projects/spadash/www-site/development/ngrok/ngrok.js|54|1|V|javascript
	  const logEvent = (keyVals) => {
	    const parser = new KeyValueParser(`${keyVals}`, { quoted: '"', delimiter: ' ' });
	    parser.on('end', async (data) => {
	      if (data.err && data.err !== 'nil') {
	        let chalkColor = 'yellow';
	        if (data.lvl !== 'info') {
	          chalkColor = data.lvl === 'warn' ? 'yellow' : 'red';
	        }
	        logLevel = data.lvl === 'info' || 'warn' ? 'warn' : 'error';
	        signale[logLevel](
	          [
	            data.status ? chalk[chalkColor].bold(data.status) : '',
	            data.msg ? chalk[chalkColor].bold(data.msg) : '',
	            chalk[chalkColor].italic(data.err),
	            data.url ? chalk.cyan(data.url) : '',
	            data.addr ? `(${data.addr})` : '',
	            data.path ? chalk.gray(data.path) : '',
	            data.r,
	            data.l,
	            // data.obj,
	            data.pg,
	          ]
	            .filter(Boolean)
	            .join(' '),
	        );
	      } else {
	        signale[data.lvl](
	          [
	            data.status ? chalk.blue(data.status) : '',
	            data.msg ? chalk.white(data.msg) : '',
	            data.url ? chalk.cyan(data.url) : '',
	            data.addr ? chalk.green(data.addr) : '',
	            data.path ? chalk.gray(data.path) : '',
	            data.r,
	            data.l,
	            // data.obj,
	            data.pg,
	          ]
	            .filter(Boolean)
	            .join(' '),
	        );
	      }
	    });
	  };
