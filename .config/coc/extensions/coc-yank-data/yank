	const showTunnelInfo = (tunnel) => {
	  // tunnel.metrics.http: count, rate1, rate5, rate15, p50, p90, p95, p99
	  // tunnel.metrics.conns: +gauge
	  console.log(tunnel);
	};
	
	const ngrokStatus = {
	  tunnels: [],
	  requests: [],
	};
	
	const showRequestInfo = (request) => {
	  console.log(`${ngrokStatus.requests.length + 1}\t${_.keys(request)}`);
	  ngrokStatus.requests.push(request);
	};
	
	const queryStatus = async (api) => {
	  const tunnelDiff = async () => {
	    const { tunnels } = await api.listTunnels();
	    const existingIds = _.map(ngrokStatus.tunnels, 'name');
	    const removed = _.difference(existingIds, _.map(tunnels, 'name'));
	    const added = _.difference(_.map(tunnels, 'name'), existingIds);
	    _.each(removed, showTunnelInfo);
	    _.each(added, showTunnelInfo);
	    ngrokStatus.tunnels = tunnels;
	  };
	
	  const requestsDiff = async () => {
	    const { requests } = await api.listRequests();
	    const added = _.difference(requests, ngrokStatus.requests);
	    _.each(added, showRequestInfo);
	  };
	
	  await tunnelDiff();
	  await requestsDiff();
	};
	
	
